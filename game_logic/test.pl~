:- [cell].
:- [gameBoard].
:- [deck].
:- [instructions].
:- [main].

testing(P) :-
        %initiate_PvsP,
        %get_bug_count(black,mosquito,Count),
        %write(Count),
        %add_cell(queenbee,white,1,3,2),
        %get_cell(cell(queenbee,white,PiledUp,Row,Column)),
        %piece_selection(black),
        %instructions(Game),
        %add_cell(ladybug,white,0,5,5),
	%add_cell(ant,white,0,4,5),
        %add_cell(ant,white,0,4,6),
        %add_cell(ant,white,0,5,4),
        %get_neighborhood(cell(ant,white,0,1,1),Ncells),
	%get_cells_list(P),
        %hamiltonian_path(cell(ladybug,white,0,5,5)),
        %cell(ant,white,0,5,4) \== cell(ant,white,0,4,6).
        cleanup_decks,
        cleanup_cells,
        %add_cell(spider,white,0,17,13),
        %add_cell(queenbee,white,0,18,12),
        %add_cell(ant,white,0,15,15),
        %add_cell(ant,white,0,14,15),
        %add_cell(ant,white,0,16,15),
        %add_cell(ant,white,0,14,16),
        %add_cell(ant,white,0,15,16),
        %add_cell(ant,white,0,15,14),
        %add_cell(ant,white,0,16,14),
        %is_surrounded(cell(queenbee,white,0,18,12)),
        %start
        add_cell(ladybug,black,0,16,13),
        add_cell(queenbee,black,0,15,14),
        add_cell(queenbee,white,2,17,13),
        add_cell(ant,white,0,18,12),
        add_cell(spider,white,0,19,12),
        add_cell(grasshopper,white,0,16,15),
        add_cell(ant,white,2,15,15),
        add_cell(beetle,white,1,15,15),
        add_cell(beetle,black,0,15,15),
        add_cell(beetle,black,1,17,13),
        add_cell(mosquito,black,0,17,13),
        add_cell(pillbug,black,0,17,14),
        add_deck(white,1,3,0,1,0,1,1,1),
        add_deck(black,3,2,1,2,0,0,0,0),
        %stunne_cell(cell(pillbug,black,0,17,14),2),
        stunne_cell(cell(grasshopper,white,0,16,15),1),
        %is_stunned(cell(pillbug,black,0,17,14)),
        %\+is_stunned(cell(beetle,black,1,17,13)),
        update_stunned_pieces,
        %is_stunned(cell(pillbug,black,0,17,14)),
        %\+is_stunned(cell(cell(mosquito,black,0,17,13))),
        %last_cell_placed(cell(pillbug,black,0,17,14)),
        %is_last(cell(pillbug,black,0,17,14)),
        %\+is_last(cell(mosquito,black,0,17,13)),
        %update_last_cell(cell(mosquito,black,0,17,13)),
        %is_last(cell(mosquito,black,0,17,13)),
        %is_last(cell(pillbug,black,0,17,14)),
        play_game(black,11).

